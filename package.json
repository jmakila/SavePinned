{
  "name": "savepinned",
  "version": "1.1.2",
  "description": "A chrome extension to save and load groups of pinned tabs",
  "main": "background.js",
  "scripts": {
    "create-profile-chromium": "chromium --user-data-dir=savepinned-chromium --first-run",
    "create-profile-firefox": "firefox -CreateProfile savepinned-firefox",
    "run-chromium": "web-ext run -t chromium --keep-profile-changes --pre-install --chromium-profile savepinned-chromium",
    "run-firefox": "web-ext run -t firefox-desktop --keep-profile-changes --pre-install --firefox-profile savepinned-firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmakila/SavePinned.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmakila/SavePinned/issues"
  },
  "homepage": "https://github.com/jmakila/SavePinned",
  "devDependencies": {
    "web-ext": "^5.5.0"
  }
}
